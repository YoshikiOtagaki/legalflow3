# LegalFlow3 - Update Case Function Environment Variables
# Example environment variables for updateCase Lambda function

# AWS Configuration
AWS_REGION=us-east-1
NODE_ENV=production

# DynamoDB Table Names
CASES_TABLE_NAME=LegalFlow3-Cases
CASE_ASSIGNMENTS_TABLE_NAME=LegalFlow3-CaseAssignments

# Lambda Configuration
FUNCTION_NAME=LegalFlow3-updateCase
FUNCTION_VERSION=$LATEST
MEMORY_SIZE=256
TIMEOUT=30

# Logging Configuration
LOG_LEVEL=INFO
CLOUDWATCH_LOGS_GROUP=/aws/lambda/LegalFlow3-updateCase

# Security Configuration
ENCRYPTION_KEY=your-encryption-key-here
JWT_SECRET=your-jwt-secret-here

# Performance Configuration
MAX_CONCURRENT_EXECUTIONS=100
RESERVED_CONCURRENCY=10

# Monitoring Configuration
XRAY_TRACING_ENABLED=true
METRICS_NAMESPACE=LegalFlow3/UpdateCase

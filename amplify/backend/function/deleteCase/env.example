# LegalFlow3 - Delete Case Function Environment Variables
# Example environment variables for deleteCase Lambda function

# AWS Configuration
AWS_REGION=us-east-1
NODE_ENV=production

# DynamoDB Table Names
CASES_TABLE_NAME=LegalFlow3-Cases
CASE_ASSIGNMENTS_TABLE_NAME=LegalFlow3-CaseAssignments
CASE_PARTIES_TABLE_NAME=LegalFlow3-CaseParties
TASKS_TABLE_NAME=LegalFlow3-Tasks
TIMESHEET_ENTRIES_TABLE_NAME=LegalFlow3-TimesheetEntries
MEMOS_TABLE_NAME=LegalFlow3-Memos
SUBSCRIPTIONS_TABLE_NAME=LegalFlow3-Subscriptions

# Lambda Configuration
FUNCTION_NAME=LegalFlow3-deleteCase
FUNCTION_VERSION=$LATEST
MEMORY_SIZE=512
TIMEOUT=60

# Logging Configuration
LOG_LEVEL=INFO
CLOUDWATCH_LOGS_GROUP=/aws/lambda/LegalFlow3-deleteCase

# Security Configuration
ENCRYPTION_KEY=your-encryption-key-here
JWT_SECRET=your-jwt-secret-here

# Performance Configuration
MAX_CONCURRENT_EXECUTIONS=50
RESERVED_CONCURRENCY=5

# Monitoring Configuration
XRAY_TRACING_ENABLED=true
METRICS_NAMESPACE=LegalFlow3/DeleteCase
